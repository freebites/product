{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleMapsApiKey": "AIzaSyDF7AgfC9zzZNgYfptHcJM9iUWwU1BhSHA"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.jtan06.product"
    },
    "name": "product",
    "slug": "product",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Maps to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "googleMapsApiKey": "AIzaSyDF7AgfC9zzZNgYfptHcJM9iUWwU1BhSHA",
      "bundleIdentifier": "com.jtan06.product"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fbc364d1-bddc-4134-ac07-57fb63fb3406"
      }
    },
    "owner": "freebites7"
  }
}
